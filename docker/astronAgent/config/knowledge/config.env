# Knowledge Service Configuration
# This file contains environment variables for the Knowledge Service application

# ============================
# Serve Configuration
# ============================
SERVICE_PORT=
WORKERS=
# ============================
# Logging Configuration
# ============================
# Log level for the knowledge service (DEBUG, INFO, WARN, ERROR)
COMMON_LOG_PATH="./openstellar_log"
COMMON_LOG_LEVEL="ERROR"


# ============================
# Metrics Configuration
# ============================
# OTLP METRIC
# SDK metric reporting interval - recommended <30000ms, default 1000ms
OTLP_METRIC_EXPORT_INTERVAL_MILLIS=
# Default metrics server timeout configuration in ms, default 5000ms
OTLP_METRIC_EXPORT_TIMEOUT_MILLIS=
# Default server connection timeout in ms, default 5000ms
OTLP_METRIC_TIMEOUT=

# ============================
# Distributed Tracing Configuration
# ============================
# OTLP TRACE
OTLP_TRACE_NAME=
OTLP_TRACE_TIMEOUT=
OTLP_TRACE_MAX_QUEUE_SIZE=
OTLP_TRACE_SCHEDULE_DELAY_MILLIS=
OTLP_TRACE_MAX_EXPORT_BATCH_SIZE=
OTLP_TRACE_EXPORT_TIMEOUT_MILLIS=

# ============================
# OpenTelemetry Data Collection Configuration
# ============================
# Reporting endpoint address
OTLP_ENDPOINT=
# Service name
OTLP_SERVICE_NAME=
# Reporting region
OTLP_DC=
# Reporting enable/disable switch
OTLP_ENABLE=1

# OTLP SID
OTLP_SID_SUB=
OTLP_SID_LOCATION=
OTLP_SID_LOCAL_PORT=


# ============================
# AIUI Service Configuration
# ============================
# Repository ID for AIUI queries
AIUI_QUERY_REPOID_V2=
# AIUI service base URL
AIUI_URL_V2=
# API key for AIUI service authentication
AIUI_API_KEY=
# API secret for AIUI service authentication
AIUI_API_SECRET=

# ============================
# Xinghuo (Spark) Service Configuration
# ============================
# Xinghuo RAG service base URL
XINGHUO_RAG_URL=
# Application ID for Xinghuo service
XINGHUO_APP_ID=
# Application secret for Xinghuo service authentication
XINGHUO_APP_SECRET=
# Dataset ID for Xinghuo knowledge base
XINGHUO_DATASET_ID=
# Search overlap parameter for Xinghuo
XINGHUO_SEARCH_OVERLAP=

# ============================
# SparkDesk Service Configuration
# ============================
# SparkDesk RAG service base URL
DESK_RAG_URL=
# Application ID for SparkDesk service
DESK_APP_ID=
# API secret for SparkDesk service authentication
DESK_API_SECRET=

# ============================
# RAGFlow Service Configuration
# ============================
# RAGFlow service base URL
RAGFLOW_BASE_URL=
# API token for RAGFlow service authentication
RAGFLOW_API_TOKEN=
# Request timeout for RAGFlow operations (seconds)
RAGFLOW_TIMEOUT=
RAGFLOW_DEFAULT_GROUP=