<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.astra.console.commons.mapper.space.EnterpriseMapper">

<select id="selectByJoinUid" resultType="com.iflytek.astra.console.commons.dto.space.EnterpriseVO">
        select e.*, eu.role role from agent_enterprise e
        left join agent_enterprise_user eu on (e.id = eu.enterprise_id)
        where eu.uid = #{joinUid}
    </select>

</mapper>